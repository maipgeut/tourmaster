<wpml-config>
    <custom-fields>
        <custom-field action="translate">tourmaster-tour-option</custom-field>
        <custom-field action="copy">tourmaster-room-option</custom-field>
        <custom-field action="copy">tourmaster-room-amount</custom-field>
        <custom-field action="copy">tourmaster-room-date</custom-field>
        <custom-field action="copy">tourmaster-room-date-avail</custom-field>
        <custom-field action="copy">tourmaster-room-date-display</custom-field>
        <custom-field action="copy">tourmaster-room-max-guest</custom-field>
        <custom-field action="copy">tourmaster-room-min-guest</custom-field>
        <custom-field action="copy">tourmaster-room-bed-type</custom-field>
        <custom-field action="copy">tourmaster-room-city-tax</custom-field>
        <custom-field action="translate">tourmaster-room-custom-excerpt</custom-field>
        <custom-field action="ignore">tourmaster-room-custom-excerpt</custom-field>
    </custom-fields>
    <custom-fields-texts>
        <key name="tourmaster-tour-option">
            <key name="promo-text"/>
            <key name="duration-text"/>
            <key name="date-range"/>
            <key name="departure-location"/>
            <key name="return-location"/>
        </key>
    </custom-fields-texts>
    <admin-texts>
        <key name="tourmaster_general">
            <key name="admin-booking-made-mail-title"></key>
            <key name="admin-booking-made-mail"></key>
            <key name="admin-guest-booking-made-mail-title"></key>
            <key name="admin-guest-booking-made-mail"></key>
            <key name="admin-payment-submitted-mail-title"></key>
            <key name="admin-payment-submitted-mail"></key>
            <key name="admin-online-payment-made-mail-title"></key>
            <key name="admin-online-payment-made-mail"></key>
            <key name="registration-complete-mail-title"></key>
            <key name="registration-complete-mail"></key>
            <key name="booking-made-mail-title"></key>
            <key name="booking-made-mail"></key>
            <key name="guest-booking-made-mail-title"></key>
            <key name="guest-booking-made-mail"></key>
            <key name="payment-made-mail-title"></key>
            <key name="payment-made-mail"></key>
            <key name="booking-cancelled-mail-title"></key>
            <key name="booking-cancelled-mail"></key>
            <key name="booking-reject-mail-title"></key>
            <key name="booking-reject-mail"></key>
            <key name="enquiry-form-fields"></key>
            <key name="additional-traveller-fields"></key>
            <key name="contact-detail-fields"></key>
            <key name="register-term-of-service-page"></key>
            <key name="register-privacy-statement-page"></key>
            <key name="user-navigation-bottom-text"></key>
        </key>
        <key name="tourmaster_room_general">
            <key name="admin-booking-made-mail-title"></key>
            <key name="admin-booking-made-mail"></key>
            <key name="admin-guest-booking-made-mail-title"></key>
            <key name="admin-guest-booking-made-mail"></key>
            <key name="admin-payment-submitted-mail-title"></key>
            <key name="admin-payment-submitted-mail"></key>
            <key name="admin-online-payment-made-mail-title"></key>
            <key name="admin-online-payment-made-mail"></key>
            <key name="booking-approve-mail-title"></key>
            <key name="booking-approve-mail"></key>
            <key name="booking-made-mail-title"></key>
            <key name="booking-made-mail"></key>
            <key name="booking-made-approval-mail-title"></key>
            <key name="booking-made-approval-mail"></key>
            <key name="guest-booking-made-mail-title"></key>
            <key name="guest-booking-made-mail"></key>
            <key name="payment-made-mail-title"></key>
            <key name="payment-made-mail"></key>
            <key name="booking-cancelled-mail-title"></key>
            <key name="booking-cancelled-mail"></key>
            <key name="booking-reject-mail-title"></key>
            <key name="booking-reject-mail"></key>
            <key name="enquiry-form-fields"></key>
            <key name="enquiry-mail-title"></key>
            <key name="enquiry-mail-content"></key>
            <key name="additional-guest-fields"></key>
            <key name="contact-detail-fields"></key>
        </key>
        <key name="tourmaster_page_option">
            <key name="extra-booking-info"></key>
            <key name="contact-detail-fields"></key>
            <key name="enquiry-form-fields"></key>
            <key name="enquiry-form-mail-content-admin"></key>
            <key name="enquiry-form-mail-content-customer"></key>
        </key>
        <key name="tourmaster_custom_tour_taxs">
            <key name="*">
                <key name="label" />
            </key>
        </key>
    </admin-texts>
</wpml-config>
